@{
    Layout = "~/Views/Shared/_Layout_List.cshtml";
}

<div id="product_images_wrapper"></div>
<button id="upload">
    上传
</button>
<script src="~/Scripts/vendor/exif/exif.min.js"></script>
<script src="~/Scripts/vendor/html5Uploader/html5Uploader.js"></script>
<script>
    var productImageUploader = new Html5Uploader("#product_images_wrapper", {
        maxHeight: 100,
        maxWidth: 100,
        uploadLimit: 2
    });
    $(function () {
        $('#upload').click(function () {
            var images = productImageUploader.getUploadFiles();
            console.log(images);
        });
    });
</script>